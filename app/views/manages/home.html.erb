<h1>組織ID作成</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:session, url: login_path) do |f| %>

      <%= f.label :email %>
      <%= f.email_field :email, class: 'form-control' %>

      <%= f.label :password %>
      <%= f.password_field :password, class: 'form-control' %>
      
    　<%= f.label :customer %>
      <%= f.password_field :customer, class: 'form-control' %>

      <%= f.submit "組織ID作成", class: "btn btn-primary" %>
    <% end %>

  </div>
</div>
